{
  "root": true,
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaFeatures": {
        "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": [
      "standard"
  ],
  "plugins": [
      "react"
  ],
  "rules": {
    "no-console": "off",
    "no-debugger": "off"
  }
}